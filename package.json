{
  "name": "narr",
  "version": "1.0.0",
  "description": "Creates an array of size N, filled with undefined, without iterating to fill. Useful for applying higher order functions (e.g. map, reduce) to produce dense arrays of size N.",
  "scripts": {
    "compile": "babel -d dist/ src/",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha dist/test.js",
    "time": "babel-node src/time.js",
    "coverage": "nyc npm test && nyc report --reporter=html",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "examples": "babel-node src/examples.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "main": "dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cullylarson/narr.git"
  },
  "author": "Cully Larson <cully.larson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cullylarson/narr/issues"
  },
  "homepage": "https://github.com/cullylarson/narr#readme",
  "keywords": [
    "array",
    "map",
    "reduce",
    "fill",
    "functional"
  ],
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-cli": "^6.3.15",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "coveralls": "^2.11.4",
    "mocha": "^2.3.4",
    "nyc": "^4.0.1"
  }
}
